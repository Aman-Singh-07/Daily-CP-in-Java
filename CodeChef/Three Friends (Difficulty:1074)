// https://www.codechef.com/problems/THREEFR

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner input=new Scanner(System.in);
		int t=input.nextInt();
		while(t-->0){
		    int x=input.nextInt();
		    int y=input.nextInt();
		    int z=input.nextInt();
		    int max=Math.max(x,Math.max(y,z));
		    System.out.println((max==x+y+z-max)?"yes":"no");
		}

	}
}
