// https://www.codechef.com/problems/MAKENZERO?tab=statement

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner input=new Scanner(System.in);
		int t=input.nextInt();
		while(t-->0){
		    int n=input.nextInt();
		    System.out.println((n==1 || n==2 || n==5)?"NO":"YES");
		}

	}
}
