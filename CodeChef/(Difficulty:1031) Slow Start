// https://www.codechef.com/problems/SLOWSTART

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner input=new Scanner(System.in);
		int t=input.nextInt();
		while(t-->0){
		    int x=input.nextInt();
		    int h=input.nextInt();
		    int count=0;
		    while(h>0){
		        if(count>=5){
		            h=h-x;
		        }
		        else{
		            h=h-(x/2);
		        }
		        count++;
		    }
		    System.out.println(count);
		}

	}
}
