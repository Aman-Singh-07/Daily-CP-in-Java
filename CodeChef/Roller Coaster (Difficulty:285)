// https://www.codechef.com/problems/MINHEIGHT

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner input=new Scanner(System.in);
		int t=input.nextInt();
		while(t-->0){
		    int x=input.nextInt();
		    int h=input.nextInt();
		    System.out.println(x<h?"NO":"YES");
		}

	}
}
