// https://www.codechef.com/problems/CHEFTRANS

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner input=new Scanner(System.in);
		int t=input.nextInt();
		while(t-->0){
		    long x=input.nextInt();
		    long y=input.nextInt();
		    long z=input.nextInt();
		    System.out.println((x+y)==z?"EQUAL":(x+y)>z?"TRAIN":"PLANEBUS");
		}

	}
}
