// https://codeforces.com/problemset/problem/2009/A

import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner input=new Scanner(System.in);
        int t=input.nextInt();
        while(t-->0){
            int a=input.nextInt();
            int b=input.nextInt();
            int res=b-a;
            System.out.println(res);
        }
    }
}
