// https://www.geeksforgeeks.org/problems/largest-element-in-array4009/1?page=2&sortBy=submissions

class Solution {
    public static int largest(int[] arr) {
        // code here
        int max=0;
        for(int i=0;i<arr.length;i++){
            if(arr[i]>max) max=arr[i];
        }
        return max;
    }
}
