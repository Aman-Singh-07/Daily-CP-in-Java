// https://www.geeksforgeeks.org/problems/floor-in-a-sorted-array-1587115620/1?page=1&difficulty=Easy&sortBy=submissions

class Solution {

    static int findFloor(int[] arr, int x) {
        // write code here
        int res=-1;
        for(int i=0;i<arr.length;i++){
            if(arr[i]<=x){
                res=i;
            }
        }
        return res;
    }
}
